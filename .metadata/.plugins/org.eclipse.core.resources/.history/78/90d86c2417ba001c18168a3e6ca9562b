package day0412;

import java.util.Scanner;

public class day0412_3 {

	public static void main(String[] args) {

		int[] arr = { 3, 2, 1, 5, 4 };
		int temp = 0;
		
		System.out.println("¹è¿­Àü");
		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] > arr[j]) {
					temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}
			}
		}
		for(int i =0; i<arr.length; i++) {
			System.out.print(arr[i]);
		}

	}
}