package test;

import java.util.Scanner;

public class test001 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		for(int i = 1; i <= 9; i++) {
			for(int j = 2; j <= 9; j++) {
				System.out.print(j + "x" + i + "=" + (j*i) + "\t");
			}
			System.out.println();
		}
	}
}